    "@types/jsonwebtoken": "^7.2.3",
    "@types/lodash": "^4.14.74",
    "@types/mongoose": "^4.7.21",
    "@types/mongoose-unique-validator": "^1.0.0",
    "@types/morgan": "^1.7.32",
    "@types/nodemailer": "^3.1.3",
    "@types/passport": "^0.3.4",
    "@types/passport-local": "^1.0.31",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.5",
